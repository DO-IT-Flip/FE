{
  "compilerOptions": {
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@container/*": ["src/components/containers/*"],
      "@components/Layout/*": ["src/components/Layout/*"],
      "@components/Modal/*": ["src/components/Modal/*"],
      "@assets/*": ["src/assets/*"],
      "@styles/*": ["src/assets/styles/*"],
      "@icons/*": ["src/assets/icons/*"],
      "@logo/*": ["src/assets/logo/*"],
      "@hooks/*": ["src/hooks/*"],
      "@data/*": ["src/data/*"],
      "@mocks/*": ["src/mocks/*"],
      "@types/*": ["src/types/*"],
      "@pages/*": ["src/pages/*"],
      "@types/event": ["src/types/event"],
      "@types/tag": ["src/types/tag"],
      "@types/user": ["src/types/user"],
      "@types/day": ["src/types/day"]
    },
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src", "src/assets", "src/types"]
}
